package com.rajendra.pkg2

object Lab07 {
  class Matcher {

    def activity(day: String) = {
      day match {
      case "saturday" => "go out with friends"
      case "sunday" => "sleep"
      case _ => "work"
    }
    }
  }

  def describe(x: Any) = x match {
    // FIXME
    case 1 => "one"
    case 2 => "two"
    case _ => "many"
  }
}